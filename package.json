{
    "name": "@openaip/yaixm-to-geojson",
    "version": "1.12.3",
    "description": "Converts YAIXM to GeoJSON. YAIXM is a simplified version of the FAA/EUROCONTOL Aeronautical Information Exchange Model (AIXM) using YAML.",
    "keywords": [
        "openaip",
        "yaixm",
        "aixm",
        "format",
        "geosjon"
    ],
    "authors": [
        {
            "name": "Stephan Besser",
            "email": "stephan@openaip.net",
            "homepage": "https://www.openaip.net",
            "role": "Lead developer"
        }
    ],
    "engines": {
        "node": ">=22.18.0"
    },
    "type": "module",
    "main": "./dist/index.js",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "default": "./dist/index.js"
        },
        "./*": {
            "types": "./dist/*.d.ts",
            "import": "./dist/*.js",
            "default": "./dist/*.js"
        }
    },
    "files": [
        "dist"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/openAIP/openaip-yaixm-to-geojson.git"
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com/"
    },
    "bugs": {
        "url": "https://github.com/openAIP/openaip-yaixm-to-geojson/issues"
    },
    "homepage": "https://github.com/openAIP/openaip-yaixm-to-geojson#readme",
    "scripts": {
        "build": "npm run clean && tsc -p tsconfig.json",
        "clean": "rimraf dist",
        "sca:watch:types": "npx chokidar '**/*.js' '**/*.ts' -c 'npm run tsc:run'",
        "sca:watch:lint": "npx chokidar '**/*.js' '**/*.ts' -c 'npm run lint'",
        "test": "vitest run --coverage",
        "tsc:run": "tsc --noEmit",
        "lint": "eslint src --cache",
        "format": "prettier --write .",
        "release:patch": "npm version --sign-git-tag patch && git push && git push origin --tags",
        "release:minor": "npm version --sign-git-tag minor && git push && git push origin --tags",
        "release:major": "npm version --sign-git-tag major && git push && git push origin --tags",
        "run-security-audit": "npm audit --omit=dev",
        "update-packages": "ncu --target minor --upgrade && npm install && npm audit fix",
        "depcheck": "npx depcheck --oneline --skip-missing=true --ignore-patterns=dist,coverage,*.log"
    },
    "dependencies": {
        "@openaip/coordinate-parser": "^0.24.2",
        "@turf/turf": "^7.2.0",
        "ajv": "^8.17.1",
        "ajv-errors": "^3.0.0",
        "yaml": "^2.8.1",
        "zod": "^3.25.76"
    },
    "devDependencies": {
        "@eslint/js": "^9.35.0",
        "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
        "@types/node": "^22.18.5",
        "@vitest/coverage-v8": "^3.2.4",
        "@vitest/ui": "^3.2.4",
        "commander": "^13.1.0",
        "eslint": "^9.35.0",
        "eslint-plugin-security": "^3.0.1",
        "npm-check-updates": "^17.1.18",
        "prettier": "^3.6.2",
        "rimraf": "^6.0.1",
        "typescript": "<=5.7.3",
        "typescript-eslint": "^8.44.0",
        "vite": "^6.3.6",
        "vitest": "^3.2.4"
    }
}
